./array.bin 4 "1 3 5 10" 7 # 3 => 5 + 1 + 1 
./array.bin 4 "1 2 5 10" 6 # 2 => 5 + 1
./array.bin 3 "1 3 7" 37 # 7 => 7 * 5 + 1 + 1
./array.bin 3 "2 3 5" 9 # 3 => 3 + 3 + 3
./array.bin 3 "2 4 5" 7 # 2 => 5 + 2
./array.bin 3 "2 3 5" 9 # 3 => 5 + 2 + 2
./array.bin 3 "2 3 5" 1 # -1
./array.bin 3 "2 3 5" 10 # 2 => 5 + 5
./array.bin 3 "2 3 5" 11 # 3 => 5 + 3 + 3
./array.bin 3 "2 3 5" 12 # 4 => 5 + 5 + 2
./array.bin 2 "2 3" 7 # 3 => 3 + 2 + 2
./array.bin 2 "2 4 6" 3 # -1
./array.bin 7 "2 4 6 7 8 9 10" 5 # -1
./array.bin 5 "2 4 7 9 10" 8 # 2 => 4 + 4
./array.bin 6 "2 4 5 7 9 10" 8 # 2 => 4 + 4
./array.bin 6 "2 4 5 7 9 10" 6 # 2 => 4 + 2
./array.bin 4 "1 3 5 10" 101 # 12 => 10 * 10 + 1
./array.bin 3 "10 9 1" 18 # 2 => 9 * 2
./array.bin 3 "1 5 7" 19 # 3
./array.bin 5 "7 8 9 10 11" 13 # -1 
